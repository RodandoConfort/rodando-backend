    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cookie-parser": "^1.4.7",
    "device-detector-js": "^3.0.3",
    "dotenv": "^16.5.0",
    "eventemitter2": "^6.4.9",
    "geoip-lite": "^1.4.10",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
