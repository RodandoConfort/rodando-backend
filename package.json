  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "typeorm:generate": "ts-node -r tsconfig-paths/register data-source.ts migration:generate",
    "typeorm:run": "ts-node -r tsconfig-paths/register data-source.ts migration:run",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "ws:test": "node scripts/ws-test.js",
    "ws:test:da:driver": "node scripts/ws-da-driver.js",
    "ws:test:da:admin": "node scripts/ws-da-admin.js",
    "ws:test:trip:passenger": "node scripts/ws-trip-passenger.js",
    "ws:test:trip:flow": "node scripts/ws-test-trip-flow.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
